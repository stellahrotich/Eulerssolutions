/*Problem 1 Solution*/
#include <iostream>

int main(int argc, const char * argv[]) {
    int total= 0;
    
    for(int i = 1;i<1000; i++ ){
        if(i%3 == 0){
            total += i;
            
        }else if(i%5 == 0){
            total +=i;
            
        }
    }
    
    std::cout << total;
    return 0;
}
